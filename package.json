{
  "name": "node-fades-discord",
  "version": "0.0.1",
  "description": "Fades Discord Bot but it's written in typescript",
  "main": "index.ts",
  "repository": "github:danon4ik/node-fades-discord",
  "author": "Danon <fadescode@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production node dist",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register index.ts",
    "build": "rm -rf ./dist && tsc && tscpaths -p tsconfig.json -s ./ -o ./dist"
  },
  "dependencies": {
    "discord.js": "^12.5.3",
    "node-fetch": "^2.6.1",
    "slash-create": "^3.2.3",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.2",
    "typescript-json-schema": "^0.50.1"
  }
}
